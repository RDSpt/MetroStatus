<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.Pane?>

<VBox prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.javafx.Controller">
   <children>
    <AnchorPane style="-fx-background-color: #fffa97">
         <children>
             <Label fx:id="amarelaStatus" layoutX="50.0" prefHeight="50.0" text="Label" />
            <AnchorPane fx:id="amarelaAnchor" style="-fx-background-color: #fffa97;">
               <children>
                   <ImageView fx:id="amarelaImg" blendMode="MULTIPLY" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                      <image>
                          <Image url="@../../graphics/amarela.png" />
                      </image>
                   </ImageView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
       <AnchorPane style="-fx-background-color: #9ec0ed;">
         <children>
             <Label fx:id="azulStatus" layoutX="50.0" prefHeight="50.0" text="Label" />
            <AnchorPane fx:id="azulAnchor" style="-fx-background-color: #9ec0ed;">
               <children>
                   <ImageView fx:id="azulImg" blendMode="MULTIPLY" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                      <image>
                          <Image url="@../../graphics/azul.png" />
                      </image>
                   </ImageView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      <AnchorPane style="-fx-background-color: #83c7a0;">
         <children>
             <Label fx:id="verdeStatus" layoutX="50.0" prefHeight="50.0" text="Label" />
            <AnchorPane fx:id="verdeAnchor" style="-fx-background-color: #83c7a0;">
               <children>
                   <ImageView fx:id="verdeImg" blendMode="MULTIPLY" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                      <image>
                          <Image url="@../../graphics/verde.png" />
                      </image>
                   </ImageView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      <AnchorPane style="-fx-background-color: #e08d8e;">
         <children>
             <Label fx:id="vermelhaStatus" layoutX="50.0" prefHeight="50.0" text="Label" />
            <AnchorPane fx:id="vermelhaAnchor" style="-fx-background-color: #e08d8e;">
               <children>
                   <ImageView fx:id="vermelhaImg" blendMode="MULTIPLY" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                      <image>
                          <Image url="@../../graphics/vermelha.png" />
                      </image>
                   </ImageView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      <AnchorPane>
         <children>
             <Label prefHeight="25.0" prefWidth="90.0" text=" Last updated at:" />
             <Label fx:id="timestamp" layoutX="90.0" prefHeight="25.0" prefWidth="210.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
